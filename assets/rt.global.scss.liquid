// Material design INPUT
.group-input {
  position: relative;
  margin: 30px 0 24px;

  input, .input {
    color: inherit;
    padding: 10px 10px 10px 5px!important;
    display: block;
    width: 100%;
    border: none;
    border-bottom: 1px solid {{settings.b_tables_border}};
    background: transparent;
    margin-bottom: 0;
    height: 36px;
  }
  input:focus, .input:focus {
    outline: none;
  }
  label {
    color: inherit;
    font-weight: normal;
    position: absolute;
    pointer-events: none;
    margin: 0;
    left: 5px;
    top: 10px;
    transition: top 0.2s ease-out;
    -moz-transition: top 0.2s ease-out;
    -webkit-transition: top 0.2s ease-out;
  }
  button{
    border: 0;
    background: none;
    position: absolute;
    right: 5px;
    top: 10px;
    outline: none;
    color: {{settings.b_links_colour}};
    padding-left: 0;
    padding-right: 0;
    font-size: 20px;

    .zmdi:before{
      transition: 0.2s ease all;
      -moz-transition: 0.2s ease all;
      -webkit-transition: 0.2s ease all;
      content: '\f1c3';
    }
  }
  input:focus ~ label,
  input.not-empty ~ label,
  .input:focus ~ label,
  .input.not-empty ~ label{
    top: -20px;
    font-size: 90%;
    color: {{settings.b_links_colour_hover}};
  }
  input:focus ~ button,
  input.not-empty ~ button,
  .input:focus ~ button,
  .input.not-empty ~ button{
    color: {{settings.b_links_colour_hover}};

    .zmdi:before{
      content: '\f2ee';
    }
  }
  .bar {
    position: relative;
    display: block;
    width: 100%;
  }
  .bar:before,
  .bar:after {
    content: '';
    height: 2px;
    width: 0;
    bottom: 0;
    position: absolute;
    background: {{settings.b_links_colour_hover}};
    transition: width 0.2s ease-out;
    -moz-transition: width 0.2s ease-out;
    -webkit-transition: width 0.2s ease-out;
  }
  .bar:before {
    left: 50%;
  }
  .bar:after {
    right: 50%;
  }
  input:focus ~ .bar:before,
  input:focus ~ .bar:after,
  .input:focus ~ .bar:before,
  .input:focus ~ .bar:after{
    width: 50%;
  }
  .highlight {
    position: absolute;
    height: 60%;
    width: 100px;
    top: 25%;
    left: 0;
    pointer-events: none;
    opacity: 0.5;
    display: none;
  }
  input:focus ~ .highlight,
  .input:focus ~ .highlight{
    -webkit-animation: inputHighlighter 0.3s ease;
    -moz-animation: inputHighlighter 0.3s ease;
    animation: inputHighlighter 0.3s ease;
  }
}
@-webkit-keyframes inputHighlighter {
  from {
    background: {{settings.b_links_colour_hover}};
  }
  to {
    width: 0;
    background: transparent;
  }
}
@-moz-keyframes inputHighlighter {
  from {
    background: {{settings.b_links_colour_hover}};
  }
  to {
    width: 0;
    background: transparent;
  }
}
@keyframes inputHighlighter {
  from {
    background: {{settings.b_links_colour_hover}};
  }
  to {
    width: 0;
    background: transparent;
  }
}
// Material design BUTTON
.ripple{
  position: relative;
  overflow: hidden;
}
.ripple-effect {
  position: absolute;
  border-radius: 100%!important;
  -webkit-transform: scale(0);
  -ms-transform: scale(0);
  transform: scale(0);
  pointer-events: none;
  background: {{settings.btn_btn1_txt}};
  opacity: 0.25;
}
.ripple-effect.show {
  -webkit-animation: ripple .5s ease-out;
  animation: ripple .5s ease-out;
}
@-webkit-keyframes ripple { 
  to {
    -webkit-transform: scale(1.5);
    transform: scale(1.5);
    opacity: 0;
  }
}
@keyframes ripple { 
  to {
    -webkit-transform: scale(1.5);
    transform: scale(1.5);
    opacity: 0;
  }
}
// Material design CHECKBOX
.label-checkbox {
  position: relative;
  clear: both;
  margin: 0;
  cursor: pointer;
  display: inline-block;

  &:hover, &.active{
    .checkbox:before {
      opacity: 1;
    }
  }
  .checkbox {
    position: relative;
    margin: 0 7px 0 0;
    cursor: pointer;
    display: inline-block;
    width: 20px;
    height: 20px;
    vertical-align: middle;
  }
  .checkbox:before {
    content: "";
    position: absolute;
    left: 0;
    z-index: 1;
    width: 20px;
    height: 10px;
    border: 2px solid {{settings.b_links_colour_hover}};
    border-radius: 0;
    -webkit-transform: rotate(-45deg);
    -moz-transform: rotate(-45deg);
    -ms-transform: rotate(-45deg);
    -o-transform: rotate(-45deg);
    transform: rotate(-45deg);
    border-top-style: none;
    border-right-style: none;
    opacity: 0;
    -webkit-transition: all 0.2s ease-in-out;
    -moz-transition: all 0.2s ease-in-out;
    transition: all 0.2s ease-in-out;
  }
  .checkbox:checked:before {
    opacity: 1;
  }
  .checkbox:after {
    content: "";
    position: absolute;
    top: 0;
    left: 0;
    width: 20px;
    height: 20px;
    border: 1px solid {{settings.b_tables_border}};
    box-shadow: inset 0 0 1px {{settings.b_tables_border}};
    background-color: {{settings.b_scaffolding_bg}};
    cursor: pointer;
    border-radius: 0;
  }
  .name{
    vertical-align: middle;
    line-height: 20px;
  }
}