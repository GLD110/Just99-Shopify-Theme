.filters-bar{
  position: relative;
  top: 0!important;
  width: 100%;
  float: left;
  padding: 5px 10px;
  background-color: darken({{settings.b_scaffolding_bg}}, 2%);
  
  .nav_close {
    height: 55px;
    padding: 0 25px;
    background-color: darken({{settings.b_scaffolding_bg}}, 3%);
    display: none;

    button{
      font-size: 24px;
      height: 100%;
      background: none;
      border: none;
      display: inline-block;
      cursor: pointer;
      padding: 0;
      outline: none;
    }
  }
  .clearall{
    position: absolute;
    right: 10px;
  }
  .advanced-filter{
    &.size{
      .field.disable.label-checkbox{
        overflow: hidden;
        position: relative;
        border-color: {{settings.b_tables_border}}!important;

        &:after{
          background-color: {{settings.b_tables_border}};
          display: block;
          height: 2px;
          position: absolute;
          right: -8px;
          top: 16px;
          -ms-transform: rotate(-45deg);
          -webkit-transform: rotate(-45deg);
          transform: rotate(-45deg);
          width: 50px;
          content: " ";
        }
      }
    }
    .field{
      display: inline-block;
      cursor: pointer;
      text-transform: capitalize;
      
      &:hover, &.active{
        .img{
          border-color: {{settings.b_links_colour_hover}};
        }
      }
      &.active{
        .img:after{
          content: '';
          position: absolute;
          left: 0;
          top: 0;
          bottom: 0;
          right: 0;
          -webkit-box-shadow: 0 0 0 1px {{settings.btn_btn1_bg}} inset;
          -moz-box-shadow: 0 0 0 1px {{settings.btn_btn1_bg}} inset;
          box-shadow: 0 0 0 1px {{settings.btn_btn1_bg}} inset;
        }
      }
      &.disable{
        cursor: default;
        cursor: not-allowed;
        opacity: 0.6;
        
        &.label-checkbox{
          .checkbox{
            overflow: hidden;
            height: 20px;
            cursor: default;
            
            &:before{
              border-color: {{settings.b_tables_border}}!important;
              display: block;
              height: 2px;
              position: absolute;
              left: -4px;
              top: 8px;
              -ms-transform: rotate(-45deg);
              -webkit-transform: rotate(-45deg);
              transform: rotate(-45deg);
              width: 30px;
              content: " ";
              opacity: 1;
            }
            &:after{
              cursor: default;
            }
          }
        }
        .img{
          overflow: hidden;
          position: relative;
          border-color: {{settings.b_tables_border}}!important;

          &:after{
            background-color: {{settings.b_tables_border}};
            display: block;
            height: 2px;
            position: absolute;
            right: -8px;
            top: 16px;
            -ms-transform: rotate(-45deg);
            -webkit-transform: rotate(-45deg);
            transform: rotate(-45deg);
            width: 50px;
            content: " ";
          }
        }
      }
      .img{
        -webkit-transition: all 0.1s ease-out;
        -moz-transition: all 0.1s ease-out;
        transition: all 0.1s ease-out;
        display: inline-block;
        padding: 6px;
        line-height: 0;
        vertical-align: middle;
        border: 1px solid {{settings.b_tables_border}};
        box-shadow: inset 0 0 1px {{settings.b_tables_border}};
        position: relative;
        
        i{
          display: inline-block;
          width: 20px;
          height: 20px;
          background-position: center center;
          background-size: cover;
        }
      }
    }
  }
  .fields{
    margin-bottom: 0;
    max-height: 260px;
    overflow-y: auto;
    
    >li{
      display: inline-block;
      margin-right: 30px;
    }
  }
  .column{
    float: left;
    margin-left: 25px;
    
    &:first-child{
      margin-left: 0;
    }
    .filter_title{
      position: relative;
      
      span{
        text-transform: capitalize;
        cursor: pointer;
      }
      i{
        font-size: 120%;
        padding-left: 5px;
        
        &:before{
          content: '\f2f2';
        }
      }
      a{
        display: none;
        font-family: {{general_font}};
        font-size: {{settings.general_font_size | plus:0}}px;
        font-weight: normal;
        text-transform: none;
        position: absolute;
        right: 0;
        top: 0;
      }
    }
    .filter_container{
      padding: 30px 10px;
      position: absolute;
      width: 100%;
      left: 0;
      z-index: 10;
      display: none;
      top: 100%;
      background-color: {{settings.b_scaffolding_bg}};
      border-bottom: 1px solid {{settings.b_tables_border}};
      
      .filter_close{
        cursor: pointer;
        right: 10px;
        top: 15px;
        position: absolute;
        font-family: 'Material-Design-Iconic-Font';
        
        &:before{
          content: '\f136';
        }
      }
    }
  }
