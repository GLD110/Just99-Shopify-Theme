// LOGO
#site-logo{
  text-align: left;
  width: 200px;
  bottom: 9px;
  left: 0;
  position: absolute;
}
.template-logo{
  clear: both;
  display: inline-block;
}
.template-logo .retina_logo{
  display: none;
}
.header-center .template-logo{
  padding: 15px 0;
  text-align: center;
}
#header .template-logo a{
  font-size: {{settings.logo_font_size | plus:0}}px;
  font-weight: {{settings.logo_font_style}};
  color: {{settings.logo_colour}};
  display: inline-block;
  opacity: 1;
  filter: alpha(opacity=100);
  max-width: 100%;
  text-transform: none;
  line-height:1;
  font-family: {{header_font}};
  vertical-align: middle;
}
.top-bar-arrow{
  position: absolute;
  display: none;
  top: 0;
  left: 0;
  width: 100%;
  height: 26px;
  content: "";
  background-position: center center;
  background-repeat: no-repeat;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  -ms-box-sizing: border-box;
  -o-box-sizing: border-box;
  box-sizing: border-box;
  text-align: center;
  border-bottom: 1px solid {{settings.b_tables_border}};
  cursor: pointer;
  
  &:before{
    position: absolute;
    left: 50%;
    bottom: 8px;
    margin-left: -7px;
    width: 0;
    height: 0;
    content: "";
    opacity: 0.5;
  }
}
.top-bar-arrow.act:before {
  border-left: 7px solid transparent;
  border-right: 7px solid transparent;
  border-top: 8px solid {{settings.b_scaffolding_color}};
}
.top-bar-arrow:not(.act):before {
  border-left: 7px solid transparent;
  border-right: 7px solid transparent;
  border-bottom: 8px solid {{settings.b_scaffolding_color}};
}

/* ================ CART ==================== */
.terms_conditions{
  display: table;
  margin-bottom: 15px;
  
  input, label{
    display: table-cell;
    vertical-align: middle;
    line-height: 20px;
  }
  a{
    text-decoration: underline;
  }
  input{
    margin-top: 0;
    margin-bottom: 2px;
    margin-right: 5px;
  }
}
.mini-cart{
  .cart-icon {
    display: inline-block;
    position: relative;
    margin-left: 15px;
    line-height: normal;

    strong {
      display: inline-block;
      color: {{settings.b_links_colour_hover}};
      border: 2px solid {{settings.b_links_colour_hover}};
      position: relative;
      padding: 1px 8px;
      z-index: 10;
      -webkit-transition: all 200ms ease-out;
      transition: all 200ms ease-out;
    }
    .cart-icon-handle {
      border: 2px solid {{settings.b_links_colour_hover}};
      border-bottom: 0;
      right: 50%;
      margin-right: -8px;
      top: -6px;
      height: 8px;
      width: 16px;
      position: absolute;
      -webkit-border-top-left-radius: 100px;
      -webkit-border-top-right-radius: 100px;
      -moz-border-radius-topleft: 100px;
      -moz-border-radius-topright: 100px;
      border-top-left-radius: 100px;
      border-top-right-radius: 100px;
      -webkit-transition: all 200ms ease-out;
      transition: all 200ms ease-out;
    }
  }
  .nav-dropdown:after{
    right: 4px;
  }
  &:hover, &.active{
    .cart-link{
      text-decoration: underline;
      color: {{settings.b_links_colour}};
    }
    .cart-icon .cart-icon-handle {
      top: -8px;
      border-color: {{settings.btn_btn1_bg}};
    }
    .cart-icon strong{
      border-color: {{settings.btn_btn1_bg}};
      background-color: {{settings.btn_btn1_bg}};
      color: {{settings.btn_btn1_txt}};
    }
  }
  #cart-info{
    .list-unstyled{
      max-height: 265px;
      overflow-y: auto;
      padding-left: 5px;
      margin-left: -5px;
      margin-bottom: 0;

      li{
        display: table;
        width: 100%;
        margin-bottom: 10px;
        margin-left: 0;
        border-bottom: 1px solid {{settings.b_tables_border}};
        padding-bottom: 10px;

        &:last-child{
          margin-bottom: 0;
        }
        .remove{
          display: table-cell;
          vertical-align: top;
          width: 45px;
          padding-top: 10px;

          a{
            display: block;
            text-decoration: none;
            text-align: center;
            height: 34px;
            width: 34px;
            line-height: 34px;
            font-size: {{settings.social_icons_size | plus:0}}px;
            background-color: {{settings.b_states_alerts_error_bg}};
            color: {{settings.b_states_alerts_error_txt}};
            -webkit-border-radius: 50%;
            -moz-border-radius: 50%;
            border-radius: 50%;
            -webkit-transition: .2s ease-out;
            -moz-transition: .2s ease-out;
            -o-transition: .2s ease-out;
            -ms-transition: .2s ease-out;
            transition: .2s ease-out;

            &:hover{
              -webkit-box-shadow: 0 5px 11px 0 rgba(0, 0, 0, 0.18), 0 4px 15px 0 rgba(0, 0, 0, 0.15);
              -moz-box-shadow: 0 5px 11px 0 rgba(0, 0, 0, 0.18), 0 4px 15px 0 rgba(0, 0, 0, 0.15);
              box-shadow: 0 5px 11px 0 rgba(0, 0, 0, 0.18), 0 4px 15px 0 rgba(0, 0, 0, 0.15);
            }
          }
        }
        figure{
          width: 53px;
          display: table-cell;
          vertical-align: top;
        }
        .list_content{
          display: table-cell;
          vertical-align: top;

          a{
            font-size: {{settings.wrapper_title_size}}px;
            line-height: 30px;
            
            span{
              display: block;
              line-height: 20px;
            }
          }
          .quantity{
            line-height: 30px;
          }
          .amount{
            color: {{settings.product_price_colour}};
            font-size: {{settings.general_heading_size | plus:0}}px;
            font-weight: {{settings.general_heading_style}};
          }
        }
      }
    }
    .subtotal{
      text-align: center;
      margin: 0 0 10px;
      padding: 8px 0;
      font-weight: bold;
      border-top: 1px solid {{settings.b_tables_border}};
    }
    .buttons{
      text-align: center;

      .btn-1{
        margin-left: 0;
        margin-bottom: 14px;
      }
    }
  }
  .cart-loading{
    &.is_loading{
      .icon_spinner{
        display: block;
      }
      .pace{
        display: block;
      }
    }
    .pace-activity{
      position: absolute;
    }
    .icon_spinner{
      position: absolute;
      top: 0!important;
      left: 0;
      width: 100%;
      height: 100%;
      display: none;

      .mask{
        position: absolute;
      }
      .spinner{
        top: 50%;
        left: 50%;
      }
    }
  }
}
