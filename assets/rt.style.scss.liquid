html{
  margin:0!important;
  padding:0!important;
}
body.ctl-active {
  overflow: hidden;
}
body.ctl-active #page,
body.cbox-active #page{
  -webkit-filter: blur(3px);
  -moz-filter: blur(3px);
  -o-filter: blur(3px);
  -ms-filter: blur(3px);
  filter: blur(3px);
}
.box_wrapper{
  margin: 0px auto;
  overflow: hidden;
  -webkit-box-shadow: 0 0 12px rgba(0, 0, 0, 0.15);
  -moz-box-shadow:	0 0 12px rgba(0, 0, 0, 0.15);
  -ms-box-shadow:		0 0 12px rgba(0, 0, 0, 0.15);
  -o-box-shadow:		0 0 12px rgba(0, 0, 0, 0.15);
  box-shadow:			0 0 12px rgba(0, 0, 0, 0.15);
  position: relative;
}
#page{
  padding:0!important;
  background-color: {{settings.b_scaffolding_bg}};
  &.box{
    position: relative;
    z-index:15;
  }
  .central{
    width: 640px;
    padding: 0 30px;
    margin: 0 auto;
    text-align: center;
  }
}
/* ================ HEADER ==================== */
#slidingbar-wrap{
  position: relative;
  z-index: 15;
  line-height: normal;
  color: {{settings.sliding_colour}};
  
  a{
    color: {{settings.sliding_colour}};
  }
  
  #slidingbar-area{
    z-index: 99999;
    position: absolute;
    top: 0;
    right: 0;
    width: 100%;
    margin-left: auto;
    margin-right: auto;
    
    #slidingbar{
      display: none;
      position: relative;
      background-color: {{settings.sliding_bg}};
      padding: 35px 0;
    }
    .sb-toggle-wrapper{
      position: absolute;
      right: 0;
      border-top: 35px solid {{settings.sliding_bg}};
      border-left: 35px solid transparent;
      cursor: pointer;

      .sb-toggle{
        font-family: 'Material-Design-Iconic-Font';
        position: absolute;
        font-size: 0.846em;
        top: -28px;
        left: -15px;
        color: lighten({{settings.sliding_colour}}, 35%);
        font-weight: bold;

        &:before{
          content: '\f278';
        }
      }
    }
    .contact_info a{
      color: lighten({{settings.sliding_colour}}, 19%);
    }
    a:hover{
      color: lighten({{settings.sliding_colour}}, 50%);
    }
    .group-input{ margin: 15px 0; }
    .group-input input, .group-input .input{
      border-color: {{settings.sliding_colour}};
    }
    .feedback_notify{ padding-top: 15px; display: none; }
    #widget-social{ margin-top: 0; }
    .social_info .social-icons{
      .social-icon{
        border-color: lighten({{settings.sliding_colour}}, 19%);
        color: lighten({{settings.sliding_colour}}, 19%);
        
        &:hover{
          border-color: lighten({{settings.sliding_colour}}, 50%);
          color: invert(lighten({{settings.sliding_colour}}, 50%));
          background-color: lighten({{settings.sliding_colour}}, 50%);
        }
      }
    }
    .slidingbar-widget > h5{
      color: lighten({{settings.sliding_colour}}, 35%);
    }
    .slidingbar-widget{
      margin-bottom: 30px;
      
      &:last-child{
        margin-bottom: 0;
      }
    }
    .list_info ul li{
      &:first-child{padding-top:0;}
      &:last-child{padding-bottom:0;}
      a:after{background-color:lighten({{settings.sliding_colour}}, 50%);}
    }
    .text_info{
      a{
        position: relative;
        max-width: 100%;
        overflow: hidden;
        display: block;
        text-align: center;
        
        img {
          -webkit-transition: transform 0.2s ease-out;
          -moz-transition: transform 0.2s ease-out;
          -o-transition: transform 0.2s ease-out;
          -ms-transition: transform 0.2s ease-out;
          transition: transform 0.2s ease-out;
        }
        &:hover {
          img {
            -webkit-transform: scale(1.15);
            -ms-transform: scale(1.15);
            transform: scale(1.15);
            opacity: 0.8;
          }
          .caption {
            opacity: 1;
          }
        }
      }
      .caption {
        position: absolute;
        left: 0;
        bottom: 40px;
        width: 100%;
        color: {{settings.sliding_colour}};
        text-transform: uppercase;
        letter-spacing: 0.05em;
        z-index: 5;
        padding: 10px 0;
        opacity: 0;
        -webkit-transition: all .25s ease-out;
        -moz-transition: all .25s ease-out;
        -ms-transition: all .25s ease-out;
        -o-transition: all .25s ease-out;
        transition: all .25s ease-out;
        
        &:hover:before {
          opacity: 1;
        }
        &:before {
          content: '';
          position: absolute;
          left: 0;
          top: 0;
          z-index: -1;
          width: 100%;
          height: 100%;
          background: {{settings.sliding_bg}};
          opacity: 0.9;
          -webkit-transition: all .25s ease-out;
          -moz-transition: all .25s ease-out;
          -ms-transition: all .25s ease-out;
          -o-transition: all .25s ease-out;
          transition: all .25s ease-out;
        }
        h2 {
          margin: 5px 0;
          font-weight: 700;
        }
      }
    }
  }
  &.is_sliding{
    .sb-toggle:before{
      content: '\f136'!important;
    }
  }
}

#header .header-top a{
  font-size: 12px;
}
#header .header-top{
  font-family: {{header_font}};
  background-color: {{settings.top_bg}};
  color: {{settings.top_color}};
  text-align: center;
  font-size: 12px;
  padding: 9px 0 6px;
  letter-spacing: 0.05em;
  position: relative;
  z-index: 5;
  display: none;
  
  .content{
    position: relative;
  }
  a{
    color: {{settings.top_color}};
    &.link{
      border-bottom: 1px solid {{settings.top_color}};
      margin-left: 3px;
      &:hover{
        border-bottom: none;
      }
    }
  }
  .highlight{
    background-color: #FF4081;
    color: #fff;
    padding: 0 3px;
  }
  .close{
    position: relative;
    display: block;
    font-size: 16px;
    opacity: 1;
    margin-top: 2px;
  }
}
#header.sticky-header{
  &.sticky-header-hide{
    #site-navigation{
      -webkit-transform: translateZ(0) translateY(-110%);
      -moz-transform: translateZ(0) translateY(-110%);
      transform: translateZ(0) translateY(-110%);
    }
  }
  .is-sticky{
    .logo_sticky{ display: block; }
    #site-navigation{
      position: fixed;
      left: 0;
      right: 0;
      top: 0!important;
      z-index: 999;
      padding: 0;
      width: 100%;
      transition: transform .5s ease-in-out,transform .5s ease-in-out,background .5s ease-in-out;
      -moz-transition: height .5s ease-in-out,-moz-transform .5s ease-in-out,background .5s ease-in-out;
      -webkit-transition: height .5s ease-in-out,-webkit-transform .5s ease-in-out,background .5s ease-in-out;
      -o-transition: height .5s ease-in-out,-o-transform .5s ease-in-out,background .5s ease-in-out;
      -webkit-animation-duration: 0.5s;
      -moz-animation-duration: 0.5s;
      -o-animation-duration: 0.5s;
      animation-duration: 0.5s;
    }
    .pc_more .more-ctl, .pc_more .mini-cart{
      display: inline-block;
    }
  }
}
@-webkit-keyframes fadeInDown {
  0% {
    opacity: 0;
    -webkit-transform: translate3d(0, -100%, 0);
    transform: translate3d(0, -100%, 0);
  }
  100% {
    opacity: 1;
    -webkit-transform: none;
    transform: none;
  }
}
@keyframes fadeInDown {
  0% {
    opacity: 0;
    -webkit-transform: translate3d(0, -100%, 0);
    -ms-transform: translate3d(0, -100%, 0);
    transform: translate3d(0, -100%, 0);
  }
  100% {
    opacity: 1;
    -webkit-transform: none;
    -ms-transform: none;
    transform: none;
  }
}
.fadeInDown {
  -webkit-animation-name: fadeInDown;
  animation-name: fadeInDown;
}
