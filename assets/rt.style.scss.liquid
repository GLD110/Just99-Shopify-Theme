.theme-ctl {
  position: fixed;
  overflow-x: auto;
  overflow-y: scroll;
  -webkit-overflow-scrolling: touch;
  left: -150%;
  top: 0!important;
  width: 100%;
  height: 100%;
  z-index: 10001;

  .overlay{
    background-color: {{settings.b_scaffolding_bg}};
    opacity: 0.7;
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: -1;
  }
  &.unreveal {
    -webkit-animation: unreveal 250ms ease-out;
    animation: unreveal 250ms ease-out;
  }
  &.reveal {
    left: 0;
    -webkit-animation: reveal 300ms ease-out;
    animation: reveal 300ms ease-out;
  }
  .inner{
    padding: 70px 0 50px;
    text-align: center;
    position: relative;
    min-height: 100%;
  }
  .wrapper{
    display: inline-block;
    width: 670px;
    padding: 0 90px 0;
    position: relative;
  }
  .central{
    padding: 0 50px;
    
    .widget{
      background-color: {{settings.b_scaffolding_bg}};
      padding: 30px;
      width: 100%;
      display: table;
      box-shadow: 2px 2px 10px 2px rgba(0,0,0,0.1);

      .left{
        display: table-cell;
        vertical-align: top;
        width: 75px;
        text-align: center;
      }
      .right{
        display: table-cell;
        vertical-align: top;
        text-align: left;
        padding-left: 30px;
      }
      .product-title{
        margin: 0;
        margin-bottom: 5px;
        font-size: {{settings.wrapper_title_size | plus:0}}px;
        line-height: 30px;
      }
    }
  }
  .ctl-actions {
    position: absolute;
    z-index: 50;
    top: 20px;
    left: 0;
    width: 100%;
    text-align: center;

    a {
      font-size: 40px;
      color: {{settings.b_scaffolding_color}};
    }
  }
  .theme-gallery {
    overflow: hidden;
    white-space: nowrap;
    cursor: initial;
    height: 100%;

    .swiper-container {
      height: 100%;
    }
    .theme-images {
      position: relative;
      height: 100%;

      .theme-img, img{
        height: 100%;
      }
      .swiper-slide {
        float: left;
      }
      img {
        width: auto;
        max-width: none;
        height: 100%;
      }
    }

    .theme-thumbs {
      position: absolute;
      left: 0;
      right: 0;
      bottom: 0;
      text-align: center;
      height: 80px;
      padding-top: 20px;
      overflow-y: hidden;
      overflow-x: auto;
      margin: 0;
      z-index: 3;

      .theme-thumbs-inner {
        height: 100%;
      }
      .theme-img {
        display: inline;
        height: 100%;
        vertical-align: bottom;

        img {
          width: auto;
          max-width: none;
          height: 100%;
          vertical-align: top;
          -webkit-transition: margin 150ms,height 150ms;
          -ms-transition: margin 150ms,height 150ms;
          -moz-transition: margin 150ms,height 150ms;
          -o-transition: margin 150ms,height 150ms;
          transition: margin 150ms,height 150ms;

          &:hover {
            height: 70px;
            margin-top: -10px;
          }
        }
      }
    }
  }
}
