          transition: color 0.2s ease-in-out;
        }
        .dropdown-menu {
          min-width: 210px;
          padding: 0;
          left: 0;
          background: {{settings.b_scaffolding_bg}};

          li{
            padding: 0;
            margin: 0;
            border-top: 1px solid darken({{settings.b_scaffolding_bg}}, 5%);

            a{
              padding: 0 20px;
              line-height: 45px;
              cursor: pointer;
              display: block;
              color: {{settings.dropdown_item_colour}}!important;
              font-size: {{settings.dropdown_item_size | plus:0}}px;

              &:hover span:after{
                left: 0;
              }
              span{
                overflow: hidden;
                position: relative;

                &:after{
                  -webkit-transition: left .25s ease-out;
                  -moz-transition: left .25s ease-out;
                  -ms-transition: left .25s ease-out;
                  -o-transition: left .25s ease-out;
                  transition: left .25s ease-out;
                  content: "";
                  display: block;
                  width: 100%;
                  height: 1px;
                  position: absolute;
                  left: -100%;
                  top: 80%;
                  background-color: {{settings.dropdown_colour_hover}};
                }
              }
            }
          }

          >li {
            &:hover{
              >a{
                i{
                  transform: rotate(-90deg);
                  -ms-transform: rotate(-90deg);
                  -webkit-transform: rotate(-90deg);
                }
              }
            }
            >a{
              position: relative;
              span{
                display: table-cell;
                width: 100%;
              }
              i{
                position: absolute;
                top: 0;
                right: 20px;
                line-height: 45px;
                display: table-cell;
                font-size: 14px;
                -webkit-transition: all 0.2s ease-out;
                -moz-transition: all 0.2s ease-out;
                -o-transition: all 0.2s ease-out;
                -ms-transition: all 0.2s ease-out;
                transition: all 0.2s ease-out;
              }
            }
          }
        }
      }
      >li{
        padding: 0;
        float: left;
        -webkit-transition: none;
        -moz-transition: none;
        -o-transition: none;
        -ms-transition: none;
        transition: none;

        &:hover, &.active{
          >a:after{
            opacity: 1;
            width: 100%;
          }
        }
        &:first-child{
          >a{
            margin-left: 0;
          }
          >a:before{
            display: none;
          }
        }
        &:last-child{
          >a{
            margin-right: 0;
          }
        }
        >a{
          font-family: {{header_font}};
          color: {{settings.nav_link_colour}};
          font-weight: {{settings.nav_font_style}};
          text-transform: uppercase;
          position: relative;
          margin: 0 25px 0;
          line-height: 50px;
          display: block;
          font-size: {{settings.nav_font_size | plus:0}}px;
          -webkit-transition: none;
          -moz-transition: none;
          -o-transition: none;
          -ms-transition: none;
          transition: none;

          &:before{
            display: block;
            font-family: 'Material-Design-Iconic-Font';
            content: '\f26d';
            position: absolute;
            top: 0;
            right: auto;
            left: -27px;
            color: {{settings.nav_link_colour}};
            font-size: 4px;
          }
          &:after{
            content: '';
            position: absolute;
            left: 0;
            bottom: 0;
            width: 15%;
            height: 2px;
            background-color: {{settings.nav_link_colour}};
            -webkit-transition: all 0.33s ease-out;
            -moz-transition: all 0.33s ease-out;
            -o-transition: all 0.33s ease-out;
            -ms-transition: all 0.33s ease-out;
            transition: all 0.33s ease-out;
            opacity: 0;
          }
          i{
            padding-left: 5px;
            display: none;
          }
          >span{
            display: inline-block;

            >span{
              display: inline-block;
              .icon-bar{
                background: {{settings.nav_link_colour}};
                width: 18px;
                height: 2px;
                display: block;
                margin-top: 3px;
              }
            }
          }
        }
      }
    }
    .customer-menu-icon{
      .dropdown-menu{
        left: auto!important;
        right: 15px!important;
      }
    }
  }
}
