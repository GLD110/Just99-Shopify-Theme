{% else %}
{% assign header_font = settings.header_font_0 %}
{% endif %}

{% if settings.general_font == "1" %}
{% assign general_font = '"' | append: settings.general_font_1 | append: '"' | append: ', sans-serif' %}
{% else %}
{% assign general_font = settings.general_font_0 %}
{% endif %}

{% if settings.selector_use %}
::selection{ 
  background-color: {{settings.selector_bg}}; 
  color: {{settings.selector_colour}}; 
}
::-moz-selection{
  background-color: {{settings.selector_bg}}; 
  color: {{settings.selector_colour}}; 
}
