.nav-dropdown-parent{
  position: relative;
  
  &:before{
    position: absolute;
    content: '';
    top: 100%;
    left: 0;
    width: 100%;
    height: 15px;
    background: {{settings.b_scaffolding_bg}};
    opacity: 0;
  }
  &.active .nav-dropdown{
    display: block!important;
  }
  &:hover{
    .nav-dropdown{
      opacity: 1;
      visibility: visible;
    }
  }
  .nav-dropdown{
    display: block!important;
    opacity: 0;
    visibility: hidden;
    -webkit-transition: opacity 0.2s ease-in;
    -moz-transition: opacity 0.2s ease-in;
    -o-transition: opacity 0.2s ease-in;
    transition: opacity 0.2s ease-in;
  }
}
.nav-dropdown{
  margin-top: 5px;
  width: 330px;
  border: 3px solid {{settings.b_tables_border}};
  box-shadow: 2px 2px 10px 2px rgba(0,0,0,0.1);
  padding: 30px;
  position: absolute;
  right: 0;
  text-align: left;
  display: none;
  font-size: {{settings.general_font_size | plus:0}}px;
  background-color: {{settings.b_scaffolding_bg}};
  color: {{settings.b_scaffolding_color}};
  cursor: default;
  line-height: normal;
  
  &:after{
    top: -17px;
    border: solid transparent;
    content: ' ';
    height: 0;
    width: 0;
    position: absolute;
    pointer-events: none;
    border-color: rgba(221,221,221,0);
    border-bottom-color: {{settings.b_tables_border}};
    border-width: 7px;
    right: 17px;
  }
}
