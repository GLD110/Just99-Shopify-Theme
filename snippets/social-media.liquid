      <a target="_blank" href="{{ settings.social_icons_rss_link }}" class="social-wrapper rss">
        <span class="social-icon">
          <i class="fa fa-rss"></i>
        </span>
      </a>
    </li>
    {% endif %}
  </ul>
</div>
{% endif %}