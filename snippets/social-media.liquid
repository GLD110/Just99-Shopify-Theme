      <a target="_blank" href="{{ settings.social_icons_pinterest_link }}" class="social-wrapper pinterest">
        <span class="social-icon">
          <i class="fa fa-pinterest"></i>
        </span>
      </a>
    </li>
    {% endif %}
    {% if settings.social_icons_google_show %}
