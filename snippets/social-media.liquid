      <a target="_blank" href="{{ settings.social_icons_google_link }}" class="social-wrapper google">
        <span class="social-icon">
          <i class="fa fa-google-plus"></i>
        </span>
      </a>
    </li>
    {% endif %}
    {% if settings.social_icons_youtube_show %}
